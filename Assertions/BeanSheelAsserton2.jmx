        <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion">
          <stringProp name="BeanShellAssertion.query">
//==============================RESPONSE Code================================

String resp = prev.getResponseCode();

if (resp.equals(&quot;200&quot;)) {
    log.info(&quot;Code found: &quot; + resp);
} else {
    Failure = true;
    FailureMessage = &quot;Expected 200 but got &quot; + resp;
}

//==============================RESPONSE TIME================================

long responseTime = prev.getTime();
if (responseTime &gt; 2000) { // in milliseconds
    Failure = true;
    FailureMessage = &quot;Response time too high: &quot; + responseTime + &quot; ms&quot;;
}

//===============================RESPONSE TEXT CONTAIN========================================

String response = prev.getResponseDataAsString();
if (response.contains(&quot;Foundation&quot;)) {
    Failure = false;
    FailureMessage = &quot;Response does not contain expected text.&quot;;
}

//===============================RESPONSE TEXT CONTAIN========================================

String response = prev.getResponseDataAsString();
if (!response.contains(&quot;xyz&quot;)) {
    Failure = true;
    FailureMessage = &quot;Response does not contain expected text.&quot;;
}


//===============================CONDITION===============================================

String userType = vars.get(&quot;user_type&quot;); // e.g., &quot;admin&quot;, &quot;guest&quot; from csv file
//log.info(&quot;UUUUUUSER&quot; +userType)

String code = prev.getResponseCode();

if (userType.equals(&quot;admin&quot;) &amp;&amp; !code.equals(&quot;200&quot;)) {
   Failure = true;
    FailureMessage = &quot;Admin should get 200, got: &quot; + code;
    
} else if (userType.equals(&quot;guest&quot;) &amp;&amp; !code.equals(&quot;200&quot;)) {
    Failure = true;
    FailureMessage = &quot;Guest should get 403, got: &quot; + code;

    
}
</stringProp>
